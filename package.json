{
  "name": "@nib/menu",
  "version": "1.0.1",
  "keywords": [
    "browser",
    "menu"
  ],
  "description": "A menu written in pure HTML/JS so it can be styled nicely - unlike native ones.",
  "main": "index.js",
  "dependencies": {
    "digitaledgeit-view": "^0.1.10"
  },
  "devDependencies": {
    "consolify": "^2.0.0",
    "mocaccino": "^1.5.1",
    "mocha": "^2.2.5"
  },
  "browser": {
    "view": "digitaledgeit-view"
  },
  "scripts": {
    "build": "browserify -r ./index.js:menu -o build/build.js",
    "test": "mochify",
    "test-interactive": "browserify --plugin mocaccino --plugin consolify ./test/index.js -o build/test.html"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:nib-health-funds/menu.git"
  }
}
